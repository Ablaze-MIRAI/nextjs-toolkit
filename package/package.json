{
  "name": "@ablaze/nextjs-toolkit",
  "version": "1.0.23",
  "keywords": [
    "nextjs",
    "shadcn",
    "react",
    "web-components"
  ],
  "author": "josh.hegedus@outlook.com",
  "license": "ISC",
  "description": "A collection of Next.js/React web components",
  "bugs": {
    "url": "https://github.com/Ablaze-MIRAI/nextjs-toolkit/issues"
  },
  "homepage": "https://github.com/Ablaze-MIRAI/nextjs-toolkit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ablaze-MIRAI/nextjs-toolkit.git"
  },
  "source": "src/index.ts",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "test": "jest",
    "emit-types": "tsc -p tsconfig.build.json",
    "build:swc": "npx swc src --copy-files -d dist && npm run emit-types",
    "build:babel": "npm run clean && babel src --extensions '.ts','.tsx' --copy-files --no-copy-ignored -d dist --ignore '**/*.test.*' && npm run emit-types",
    "prune-dist": "find dist -type f -name '*.test.*' -delete",
    "clean": "rm -rf dist/",
    "prepublishOnly": "npm run clean && npm run build:babel && npm run prune-dist",
    "publish:local": "npx yalc publish",
    "format": "prettier --write '**/*.{ts,tsx}'"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@remixicon/react": "^4.2.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "framer-motion": "^11.2.6",
    "lucide-react": "^0.379.0",
    "next-themes": "^0.3.0",
    "react-aria": "^3.27.0",
    "react-stately": "^3.25.0",
    "react-use-measure": "2.1.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.16.7",
    "@babel/core": "7.16.7",
    "@swc/cli": "0.1.55",
    "@swc/core": "1.2.124",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/jest": "29.1.1",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "jest": "29.1.1",
    "jest-css-modules-transform": "4.4.2",
    "jest-environment-jsdom": "^29.1.2",
    "source-map-support": "0.5.21",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "next": ">=12.x | >=13.x",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-is": "^18.0.0",
    "styled-jsx": ">=5.x",
    "class-variance-authority": "^0.7.0"
  }
}
